
{
    "cargar_dataset": {
        "dataset": "dataset/Galaxy10_DECals.h5",
        "clases": [
            1,
            6,
            9
        ]
    },

    "parametros_top": {
        "neuronas": [
            1, 2, 3
        ],
        "capas": [
            1, 2
        ],
        "dropouts": [
            0.0, 0.1
        ],
        "activaciones": [
            "linear", "relu"
        ],
        "batch_size": 32,
        "transfer_learning": {
            "max_epoch": 50,
            "learning_rate": 0.1
        },
        "fine_tunning": {
            "max_epoch": 100,
            "learning_rate": 0.001
        },
        "test_size": 0.2,
        "seed": 8
    },

    "ejecucion": {
        "secuencial": false,
        "multihilo_clase_thread": false,
        "multihilo_pool_executor": true
    },

    "preprocesamiento": {
        "sigma": 4
    }
}


